{
  "name": "bc-24-postit",
  "version": "1.0.1",
  "description": "My postit app",
  "main": "server/build/bin/www",
  "engines": {
    "node": "6.10.2"
  },
  "scripts": {
    "start": "babel-node server/bin/www",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && nyc --reporter=html --reporter=text mocha --timeout 10000 --compilers js:babel-register server/test/**/*.test.js --exclude server/bin",
    "jest": "jest",
    "jest-coverage": "jest --coverage",
    "e2e-server": "export NODE_ENV=e2e && npm start",
    "e2e-test": "export NODE_ENV=e2e && npm run migrate:undo:all && npm run migrate && sequelize db:seed:all && nightwatch tests/acceptance/",
    "heroku-postbuild": "webpack --config ./webpack.deployment.config.js --progress --colors",
    "migrate": "sequelize db:migrate --config server/config/config.js",
    "migrate:undo:all": "sequelize db:migrate:undo:all --config server/config/config.js",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oahray/bc-24-postit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oahray/bc-24-postit/issues"
  },
  "homepage": "https://github.com/oahray/bc-24-postit#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cloudinary-core": "^2.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "jquery": "^2.2.4",
    "jsonwebtoken": "^7.4.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "node-sass": "^4.9.3",
    "nodemailer": "^4.1.0",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "randomstring": "^1.1.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-materialize": "^1.0.6",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.2.1",
    "serve-favicon": "^2.4.5",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "style-loader": "^0.18.2",
    "toastr": "^2.1.2",
    "validatorjs": "^3.13.6",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-jest": "^21.2.0",
    "coveralls": "^2.13.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "expect": "^1.20.2",
    "favicons-webpack-plugin": "0.0.7",
    "foreground-child": "^2.0.0",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^21.2.1",
    "jest-localstorage-mock": "^2.1.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-socket.io": "^1.1.0",
    "nightwatch": "^0.9.16",
    "nodemailer-mock": "^1.3.8",
    "nodemon": "^1.11.0",
    "nyc": "^11.2.1",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.3.0",
    "selenium-webdriver": "^3.6.0",
    "sequelize-cli": "^3.0.0-3",
    "sinon": "^4.1.2",
    "supertest": "^3.0.0"
  },
  "jest": {
    "verbose": true,
    "roots": [
      "client/src/"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    },
    "setupTestFrameworkScriptFile": "./client/src/__config__/setup.js",
    "collectCoverageFrom": [
      "client/src/**/*.{js|jsx}",
      "!client/src/index.js",
      "!client/src/store/index.js",
      "!client/src/__config__/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
