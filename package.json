{
  "name": "bc-24-postit",
  "version": "1.0.1",
  "description": "My postit app",
  "main": "server/build/bin/www",
  "engines": {
    "node": "6.10.2"
  },
  "scripts": {
    "start": "babel-node server/bin/www.js",
    "build": "webpack --config ./webpack.deployment.config.js --progress --colors",
    "client-start": "webpack-dev-server progress --colors",
    "prod:start": "SET \"NODE_ENV=production\" && npm start",
    "dev:start": "babel-node server/bin/www.js",
    "dev:watch": "nodemon --ignore webpack.config.js --exec \"npm run dev:start\"",
    "dev:start-template": "nodemon -e scss -x .js --ignore template/public/js, --exec \"npm run build-css & npm run build-template-js & npm start\"",
    "dev:build-template": "nodemon -e scss --ignore template/public/js --exec \"npm run build-css & npm run build-template-js\"",
    "build-template-js": "babel template/es6 -d template/public/js --presets es2015,stage-2",
    "build-css": "node-sass --include-path scss template/scss/main.scss template/public/css/main.css && node-sass --include-path scss template/scss/message.scss template/public/css/message.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha --compilers js:babel-register server/test/app-test.js",
    "test-watch": "nodemon --exec \"npm test\"",
    "heroku-postbuild": "webpack --config ./webpack.deployment.config.js --progress --colors",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oahray/bc-24-postit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oahray/bc-24-postit/issues"
  },
  "homepage": "https://github.com/oahray/bc-24-postit#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "jquery": "^2.2.4",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-materialize": "^1.0.6",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.2.1",
    "sequelize-cli": "^2.7.0",
    "socket.io": "^2.0.3",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "expect": "^1.20.2",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.29.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.1",
    "supertest": "^3.0.0"
  }
}
